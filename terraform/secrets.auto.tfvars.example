hcloud_token        = "your hcloud token"
server_name         = "your server name"
ssh_port            = 22
user_name           = ""
image               = "ubuntu-22.04"
server_type         = "cpx11"
location            = "fsn1"
public_key_path     = "~/.ssh/you public key"
private_key_path    = "~/.ssh/you private key"
additional_users    = [
  {
    groups         = "ssh-users"
    name           = "user name"
    ssh_public_key = "public key of the user"
  }
]
